{
    "compilerOptions": {
      "baseUrl": "../node_modules",
      "target": "es5",
      "lib": ["es5", "dom"],
      "types": ["cypress"],
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "module": "CommonJS",
      "moduleResolution": "Node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noImplicitAny": false,
      "sourceMap": false
    },
    "include": ["**/*.ts"],
    "supportFile": false
  }
